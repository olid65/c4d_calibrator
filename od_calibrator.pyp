import c4d,os,sys

sys.path.append(os.path.dirname(__file__))




PLUGIN_ID = 1059348 

with localimport(os.path.dirname(__file__)) as importer:
    importer.disable(['od_utils'])
    import od_utils.dlg
    import od_utils.bmp


    class MainWindows(c4d.gui.GeDialog):
        """
        This dialog contains the :class:`PoseLibrary_View` class and displays
        it in its very own window.
        """

        # Symbolic IDs for parameters in the dialog.
        ID_SCROLLGRP_BMP = 1000
        ID_USER_AREA = 1001
        ID_BTON_CHANGE_FN = 1002

        def __init__(self, fn_bmp):
            super(MainWindows, self).__init__()
            self.fn_bmp = fn_bmp
            self.bmp_area = od_utils.dlg.BmpUserArea(self)
            res = self.bmp_area.initBMP(self.fn_bmp)
            if not res :
                self.bmp_area = None

        def CreateLayout(self):
            self.SetTitle("Extracteur d'image")

            self.AddButton(self.ID_BTON_CHANGE_FN, c4d.BFH_SCALEFIT, initw=50, inith=20, name="Changer l'image")

            if self.bmp_area:
                if self.ScrollGroupBegin(self.ID_SCROLLGRP_BMP,
                                        c4d.BFH_SCALEFIT|c4d.BFV_SCALEFIT,
                                        scrollflags=c4d.SCROLLGROUP_VERT | c4d.SCROLLGROUP_HORIZ, initw=200, inith=200) :
                    self.AddUserArea(self.ID_USER_AREA, c4d.BFH_CENTER | c4d.BFV_CENTER)
                    self.AttachUserArea(self.bmp_area, self.ID_USER_AREA)

                self.GroupEnd()

            else:
                c4d.gui.MessageDialog("Probl√®me avec le fichier image")
                return False

            return True

        def Command(self, id, data):
            if id == self.ID_BTON_CHANGE_FN:
                self.fn_bmp = c4d.storage.LoadDialog(type=c4d.FILESELECTTYPE_IMAGES)
                if self.fn_bmp:
                    self.bmp_area.initBMP(self.fn_bmp)
            return True



    class CommandLuncher(c4d.plugins.CommandData):
        """
        This plugin class opens the MainWindows when it is
        selected from the Plugin's menu.
        """
        fn_img = None
        @property
        def dialog(self):
            """
            Returns the MainWindows that is managed by this command
            plugin. The dialog is generated on-demand to avoid creating it
            when it would never be opened by the user.
            """
            if not self.fn_img :
                self.fn_img = c4d.storage.LoadDialog(type=c4d.FILESELECTTYPE_IMAGES)
                if not self.fn_img: return False

            dialog = getattr(self, '_dialog', None)
            if dialog is None:
                dialog = MainWindows(self.fn_img)
                self._dialog = dialog

            return dialog

        def register(self):
            """
            Registers the plugin command to Cinema 4D.
            """

            flags = 0
            icon = None
            help_ = "Outils pour retrouver le point de vue d'une photographie depuis des points 3D"
            return c4d.plugins.RegisterCommandPlugin(
                PLUGIN_ID, "Calibrateur d'images", flags, icon, help_, self)

        def Execute(self, doc):
            return self.dialog.Open(c4d.DLG_TYPE_ASYNC, PLUGIN_ID, defaultw=100, defaulth=100)


    if __name__ == "__main__":
        # Register the Plugin Command.
        CommandLuncher().register()
    